{
  "name": "container-tab-groups",
  "version": "11.8.0",
  "description": "Chrome-like tab groups using private and isolated containers: The ultimate tab manager and groups for Firefox.",
  "main": "./dist/index.js",
  "scripts": {
    "build": "node script/clean.js && eslint . && echo \"ESLint done\" && webpack && node script/build.js",
    "watch": "webpack -w",
    "clean": "node script/clean.js && node script/clean-builds.js",
    "test": "node script/test.js"
  },
  "author": "Menhera.org",
  "license": "GPL-3.0-or-later",
  "devDependencies": {
    "@parcel/transformer-typescript-tsc": "^2.8.3",
    "@types/diff": "^5.0.3",
    "@types/glob": "^8.1.0",
    "@types/node": "^18.16.5",
    "@types/react": "^18.2.6",
    "@types/react-dom": "^18.2.4",
    "@types/webextension-polyfill": "^0.10.0",
    "@typescript-eslint/eslint-plugin": "^5.59.2",
    "@typescript-eslint/parser": "^5.59.2",
    "copy-webpack-plugin": "^11.0.0",
    "deterministic-zip-ng": "^2.0.0",
    "eslint": "^8.40.0",
    "glob": "^10.2.2",
    "markdown-it": "^13.0.1",
    "parcel": "^2.8.3",
    "parcel-resolver-ignore": "^2.1.3",
    "terser-webpack-plugin": "^5.3.8",
    "ts-loader": "^9.4.2",
    "typedoc": "^0.24.7",
    "typescript": "<5.1.0",
    "web-ext": "^7.6.2",
    "webpack": "^5.82.0",
    "webpack-cli": "^5.1.0",
    "webpack-deadcode-plugin": "^0.1.17"
  },
  "dependencies": {
    "diff": "^5.1.0",
    "webextension-polyfill": "^0.10.0",
    "weeg-containers": "^0.11.0",
    "weeg-domains": "^0.9.0",
    "weeg-events": "^0.4.0",
    "weeg-storage": "^0.5.0",
    "weeg-tabs": "^0.18.0",
    "weeg-types": "^0.4.0",
    "weeg-utils": "^0.11.0"
  },
  "browserslist": "Firefox >= 102",
  "target-background": "dist/background/background.html",
  "target-content-overrides": "dist/content/overrides.js",
  "target-content-ext-directory-listing": "dist/content/ext/directory-listing.js",
  "target-cookies": "dist/pages/cookies/cookies.html",
  "target-debugging": "dist/pages/debugging/debugging.html",
  "target-index": "dist/pages/index/index-tab.html",
  "target-navigation": "dist/pages/navigation/confirm.html",
  "target-options": "dist/pages/options/options.html",
  "target-page-action": "dist/pages/page-action/page-action.html",
  "target-panorama": "dist/pages/panorama/panorama.html",
  "target-popup-v2": "dist/pages/popup-v2/popup-v2.html",
  "targets": {
    "main": {
      "source": "src/index.ts",
      "context": "node"
    },
    "target-background": {
      "source": "src/background/background.html",
      "context": "browser"
    },
    "target-content-overrides": {
      "source": "src/content/overrides/content.ts",
      "context": "browser"
    },
    "target-content-ext-directory-listing": {
      "source": "src/content/ext/directory-listing/directory-listing.ts",
      "context": "browser"
    },
    "target-cookies": {
      "source": "src/pages/cookies/cookies.html",
      "context": "browser"
    },
    "target-debugging": {
      "source": "src/pages/debugging/debugging.html",
      "context": "browser"
    },
    "target-index": {
      "source": "src/pages/index/index-tab.html",
      "context": "browser"
    },
    "target-navigation": {
      "source": "src/pages/navigation/confirm.html",
      "context": "browser"
    },
    "target-options": {
      "source": "src/pages/options/options.html",
      "context": "browser"
    },
    "target-page-action": {
      "source": "src/pages/page-action/page-action.html",
      "context": "browser"
    },
    "target-panorama": {
      "source": "src/pages/panorama/panorama.html",
      "context": "browser"
    },
    "target-popup-v2": {
      "source": "src/pages/popup-v2/popup-v2.html",
      "context": "browser"
    }
  },
  "parcelIgnore": [
    "icon\\.svg",
    "cookies\\.svg",
    "top-sites\\.svg",
    "settings\\.svg",
    "developer\\.svg",
    ".*\\.svg",
    "react/react.production.min.js",
    "react/react-dom.production.min.js"
  ]
}
