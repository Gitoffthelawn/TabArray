{
  "manifest_version": 2,
  "name": "Container Tab Groups",
  "version": "11.4.0",
  "browser_specific_settings": {
    "gecko": {
      "id": "tab-array@menhera.org"
    }
  },
  "default_locale": "en",
  "author": "Menhera.org",
  "homepage_url": "https://github.com/menhera-org/TabArray",
  "description": "__MSG_extensionDescription__",
  "permissions": [
    "tabs",
    "tabHide",
    "<all_urls>",
    "cookies",
    "contextualIdentities",
    "menus",
    "menus.overrideContext",
    "storage",
    "browserSettings",
    "privacy",
    "webRequest",
    "webRequestBlocking",
    "sessions",
    "browsingData",
    "theme"
  ],
  "incognito": "spanning",
  "background": {
    "page": "background.html"
  },
  "browser_action": {
    "browser_style": true,
    "default_icon": {
      "16": "img/toolbar/toolbar-icon-black.svg"
    },
    "default_title": "__MSG_browserActionTitle__",
    "default_popup": "popup/popup.html",
    "default_area": "tabstrip",
    "theme_icons": [
      {
        "size": 16,
        "light": "img/toolbar/toolbar-icon-white.svg",
        "dark": "img/toolbar/toolbar-icon-black.svg"
      }
    ]
  },
  "options_ui": {
    "page": "options/options.html",
    "browser_style": true,
    "open_in_tab": false
  },
  "sidebar_action": {
    "browser_style": true,
    "default_icon": {
      "16": "icon.svg",
      "32": "icon.svg"
    },
    "default_panel": "sidebar/sidebar.html"
  },
  "commands": {
    "_execute_browser_action": {
      "suggested_key": {
        "default": "Ctrl+Alt+G"
      },
      "description": "__MSG_commandOpenPopup__"
    },
    "_execute_sidebar_action": {
      "suggested_key": {
        "default": "Ctrl+Alt+S"
      },
      "description": "__MSG_commandOpenSidebar__"
    }
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "languages/content.js"
      ],
      "all_frames": true,
      "run_at": "document_start"
    }
  ],
  "icons": {
    "48": "icon.svg",
    "96": "icon.svg"
  },
  "web_accessible_resources": [
    "navigation/confirm.html"
  ]
}
